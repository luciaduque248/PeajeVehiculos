entity IdentificacionVehiculo is
    Port (
        tag_in : in std_logic_vector(4 downto 0); -- Entrada para la identificación del vehículo
        categoria_out : out std_logic_vector(1 downto 0) -- Salida para la categoría del vehículo
    );
end IdentificacionVehiculo;

architecture Behavioral of IdentificacionVehiculo is
begin
    process (tag_in)
    begin
        case tag_in(4 downto 3) is
            when "00" =>
                categoria_out <= "00"; -- Categoría I
            when "01" =>
                categoria_out <= "01"; -- Categoría II
            when others =>
                categoria_out <= "10"; -- Categoría III
        end case;
    end process;
end Behavioral;
